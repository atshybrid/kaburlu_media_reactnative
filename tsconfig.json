{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowJs": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "files": [],
  "include": [
    "app",
    "components",
    "services",
    "context",
    "hooks",
    "config",
    "constants",
    "icons",
    "data",
    "types",
    "tests",
    "examples",
    "scripts/**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "node_modules.old",
    "node_modules_broken*",
    "build",
    "android",
    "ios",
    "temp_*.ts",
    "temp_*.tsx",
    "temp_*.*",
    "temp_fetch_*.*",
    "**/temp_*.*",
    "**/temp_fetch_*.*",
    "scratch",
    "*.config.js",
    "babel.config.js",
    "metro.config.js"
  ],
  "extends": "./node_modules/expo/tsconfig.base"
}
