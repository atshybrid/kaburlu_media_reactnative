{
  "expo": {
    "name": "kaburlu",
    "slug": "kaburlu",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "kaburlu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.media.kaburlu",
      "buildNumber": "8",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 8,
      "icon": "./assets/images/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-foreground.png",
        "backgroundImage": "./assets/images/app-icon-background.png",
        "backgroundColor": "#ff9934"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.media.kaburlu",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "s.kaburlumedia.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "kaburlumedia.com",
              "pathPrefix": "/article"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kaburlu"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "kaburlu needs access to your photos to attach images and videos to your news posts.",
          "cameraPermission": "kaburlu needs camera access to capture photos and videos for your news posts.",
          "microphonePermission": "kaburlu needs microphone access to record audio with your videos."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "kaburlu needs your location to tag news articles with your current place. This helps readers find local news in their area."
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-updates",
        {
          "checkAutomatically": "ON_LOAD"
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c7881aa5-9bf6-4eb0-a143-f6008e905458"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "google": {
        "androidClientId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "iosClientId": "YOUR_IOS_CLIENT_ID.apps.googleusercontent.com",
        "webClientId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "mode": "native"
      },
      "firebase": {
        "webApiKey": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "projectId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "appId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "messagingSenderId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "storageBucket": "__AUTO_FROM_GOOGLE_SERVICES_JSON__"
      },
      "router": {},
      "eas": {
        "projectId": "c7881aa5-9bf6-4eb0-a143-f6008e905458"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "amoghnya"
  }
}
